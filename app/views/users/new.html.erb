<h1>Sign Up</h1>
<%= form_for @user do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :confirm_password, "Confirm Password" %>
  <%= f.password_field :confirm_password %>

  <%= f.submit "Create Account" %>
<% end %>